import math
n=int(input())
count=0
while(n%2==0):
    print(2)
    count=count+1
    n=n//2
for i in range(3,int(math.sqrt(n))+1,2):
    while n%i==0:
        print(i)
        count=count+1
        n=n//i
if n>2:
    print(n)
    count=count+1
print("count",count)    



